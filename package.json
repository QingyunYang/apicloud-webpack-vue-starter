{
  "name": "apicloud-webpack-vue-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vue-cli-service serve --mode chrome",
    "dev:loader": "vue-cli-service serve --mode loader",
    "build:h5": "vue-cli-service build --mode h5",
    "build:native": "vue-cli-service build --mode native",
    "lint": "vue-cli-service lint",
    "wifi": "apicloud wifiStart --port 8686",
    "sync": "apicloud wifiSync --project ./dist --updateAll false --port 8686",
    "log": "apicloud wifiLog --port 8686"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "eruda": "^2.3.3",
    "qs": "^6.9.3",
    "querystringify": "^2.1.1",
    "store2": "^2.11.0",
    "vant": "^2.5.8",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "~4.2.0",
    "@vue/cli-plugin-eslint": "~4.2.0",
    "@vue/cli-service": "~4.2.0",
    "apicloud-cli": "^0.2.0",
    "autoprefixer": "^9.7.5",
    "babel-eslint": "^10.0.3",
    "babel-plugin-import": "^1.13.0",
    "eslint": "^6.7.2",
    "eslint-plugin-vue": "^6.1.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "postcss-px-to-viewport": "^1.1.1",
    "postcss-pxtorem": "^5.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {}
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
